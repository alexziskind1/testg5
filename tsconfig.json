{
  "compilerOptions": {

    "target": "esnext",
    "lib": ["dom", "esnext"],
    "jsx": "react",
 
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,                                 
    "skipLibCheck": true
  },
  "include": [
    "./src/**/*", 
    "./gatsby-node.ts", 
    "gatsby-config.bak", 
    "./plugins/**/*",  
    "./src/gatsby-types.d.ts",
  ]
}
